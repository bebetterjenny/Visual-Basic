Sub packingList1()


'copy from Sheet1

Dim lastRow As Integer
lastRow = Sheet1.Cells(Rows.Count, 1).End(xlUp).Row

Sheet2.Select
Sheet2.Cells.ClearContents

Sheet1.Select
Columns("C:C").Select
Selection.Copy
Sheet2.Select
Columns("A:A").Select
ActiveSheet.Paste

Sheet1.Select
Columns("D:D").Select
Selection.Copy
Sheet2.Select
Columns("B:B").Select
ActiveSheet.Paste

Sheet1.Select
Columns("E:E").Select
Selection.Copy
Sheet2.Select
Columns("C:C").Select
ActiveSheet.Paste

Sheet1.Select
Columns("G:G").Select
Selection.Copy
Sheet2.Select
Columns("D:D").Select
ActiveSheet.Paste

Sheet1.Select
Columns("N:N").Select
Selection.Copy
Sheet2.Select
Columns("E:E").Select
ActiveSheet.Paste

Sheet1.Select
Columns("S:S").Select
Selection.Copy
Sheet2.Select
Columns("F:F").Select
ActiveSheet.Paste

Sheet1.Select
Columns("T:T").Select
Selection.Copy
Sheet2.Select
Columns("G:G").Select
ActiveSheet.Paste

Sheet1.Select
Columns("U:U").Select
Selection.Copy
Sheet2.Select
Columns("H:H").Select
ActiveSheet.Paste


'sumif and remove duplicate
Range("I2").Select
ActiveCell.FormulaR1C1 = "=SUMIF(C[-8],C[-8],C[-7])"
Dim autoFillRange As String
Let autoFillRange = "I2:I" & lastRow
Range("I2").Select
Selection.AutoFill Destination:=Range(autoFillRange)

Range(Selection, Selection.End(xlDown)).Select
Selection.Copy
Range("B2").Select
Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
    :=False, Transpose:=False
Application.CutCopyMode = False

Columns("I:I").Select
Selection.ClearContents
Columns("A:H").Select
ActiveSheet.Range("$A$1:$H$69").RemoveDuplicates Columns:=1, Header:=xlNo


'new item in Sheet3
Sheet3.Select
Range("A1").Value = "sku"
Range("B1").Value = "Qty"
Range("C1").Value = "Qty/Carton"
Range("D1").Value = "Weight(lb)/Carton"
Range("E1").Value = "dimension"
Range("F1").Value = "l/case"
Range("G1").Value = "w/case"
Range("H1").Value = "h/case"
MsgBox ("Copy and paste the new item sku into the sku column. And then run packingList2.")


'vlookup from Sheet2
Sheet3.Select
Range("B2").Select
ActiveCell.FormulaR1C1 = "=VLOOKUP(Sheet3!RC1,Sheet2!C1:C8,2,0)"
Range("C2").Select
ActiveCell.FormulaR1C1 = "=VLOOKUP(Sheet3!RC1,Sheet2!C1:C8,3,0)"
Range("D2").Select
ActiveCell.FormulaR1C1 = "=VLOOKUP(Sheet3!RC1,Sheet2!C1:C8,4,0)"
Range("E2").Select
ActiveCell.FormulaR1C1 = "=VLOOKUP(Sheet3!RC1,Sheet2!C1:C8,5,0)"
Range("F2").Select
ActiveCell.FormulaR1C1 = "=VLOOKUP(Sheet3!RC1,Sheet2!C1:C8,6,0)"
Range("G2").Select
ActiveCell.FormulaR1C1 = "=VLOOKUP(Sheet3!RC1,Sheet2!C1:C8,7,0)"
Range("H2").Select
ActiveCell.FormulaR1C1 = "=VLOOKUP(Sheet3!RC1,Sheet2!C1:C8,8,0)"

lastRow = Sheet3.Cells(Rows.Count, 1).End(xlUp).Row
Dim autoFillRange As String
Let autoFillRange = "B2:H" & lastRow
Range("B2:H2").Select
Selection.AutoFill Destination:=Range(autoFillRange)



    ActiveWorkbook.Save
End Sub
